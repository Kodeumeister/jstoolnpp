{
    "name": "jstool",
    "displayName": "JSTool",
    "description": "JS (JSON) Tool for VSC",
    "version": "0.1.0",
    "publisher": "sunjw",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onCommand:extension.formatJS"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.formatJS",
            "title": "Format JS"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}